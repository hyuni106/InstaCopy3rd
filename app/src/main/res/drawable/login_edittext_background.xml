<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape>
            <corners
                android:radius="3dp" />
            <solid
                android:color="#EEEEEE" />
        </shape>
    </item>


    <item
        android:left="1dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="1dp">
        <shape>
            <corners
                android:radius="2dp" />
            <solid
                android:color="#EFEFEF" />
        </shape>
    </item>

</layer-list>